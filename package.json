{
  "name": "meetup-app",
  "version": "0.0.0",
  "description": "Lets serve up some iframes!",
  "main": "index.js",
  "scripts": {
    "watch": "npm run watch-host-js & npm run watch-iframe-js & npm run watch-server",
    "watch-host-js": "watchify host-public/js/host-app.js -o host-public/js/host-app.built.js -d -v",
    "watch-iframe-js": "watchify iframe-public/js/iframe-app.js -o iframe-public/js/iframe-app.built.js -d -v",
    "watch-server": "nodemon index.js",
    "start": "npm run watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.3.2",
    "morgan": "^1.1.1",
    "consolidate": "^0.10.0"
  },
  "devDependencies": {
    "nodemon": "^1.1.1",
    "underscore": "^1.6.0"
  }
}
